{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "prisma generate && tsc && mkdir -p dist/node_modules && cp -r node_modules/@prisma dist/node_modules/",
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development tsx src/server.ts",
    "postinstall": "prisma generate",
    "db:generate": "pnpm prisma migrate dev --name",
    "db:deploy": "pnpm prisma migrate deploy",
    "clear-images": "ts-node prisma/scripts/clearImage.ts",
    "clear-users": "ts-node prisma/scripts/clearUser.ts",
    "clear-blog-posts": "ts-node prisma/scripts/clearBlogPost.ts",
    "clear-blog-post-image": "ts-node prisma/scripts/clearBlogPostImage.ts",
    "clear-blog-comments": "ts-node prisma/scripts/clearComments.ts"
  },
  "type": "module",
  "keywords": [],
  "author": "Jordan Adkins",
  "license": "ISC",
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.1",
    "@types/express": "^5.0.1",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.13",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "form-data": "^4.0.2",
    "geoip-lite": "^1.4.10",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.2",
    "next-auth": "^4.24.11",
    "pg": "^8.18.0",
    "pnpm": "^10.30.0",
    "resend": "^4.5.1",
    "ua-parser-js": "^2.0.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/geoip-lite": "^1.4.4",
    "@types/pg": "^8.16.0",
    "@types/ua-parser-js": "^0.7.39",
    "prisma": "^7.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
